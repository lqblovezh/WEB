{
  "name": "reader3",
  "version": "3.0.0",
  "description": "web reader.",
  "main": "out/reader3.js",
  "scripts": {
    "test": "npm-run-all -p test:*",
    "test:webpack": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "test:proxy": "better-npm-run proxy",
    "test:theme": "watch --interval=0.5 \"postcss -o ./out/reader3.themes.css ./src/themes/index.css\" ./src/themes",
    "webpack": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "theme": "postcss -o ./out/reader3.themes.css ./src/themes/index.css",
    "compile": "npm-run-all clean webpack theme",
    "clean": "del-cli \"./out/*\" \"!./out/*.html\" \"!./out/polyfill.min.js\"",
    "open": "node test/open",
    "babel": "babel-node"
  },
  "betterScripts": {
    "proxy": {
      "command": "babel-watch test/proxy"
    }
  },
